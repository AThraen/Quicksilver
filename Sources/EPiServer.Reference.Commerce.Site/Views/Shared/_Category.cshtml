@using EPiServer.Reference.Commerce.Site.Features.Shared.Extensions
@model IEnumerable<EPiServer.Reference.Commerce.Site.Features.Product.Models.ProductViewModel>
@{var count = Model.Count();}
@foreach (var productModel in Model)
{
    using (Html.BeginProductSeparator(count))
    {
        Html.RenderPartial("_ProductViewModel", productModel);        
    }
}